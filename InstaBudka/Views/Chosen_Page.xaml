<Page x:Class="InstaBudka.Views.Chosen_Page"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:InstaBudka.Views"
      mc:Ignorable="d" 
      d:DesignHeight="1920" d:DesignWidth="1080"
      Title="Chose_Page"
      DataContext="{Binding RelativeSource= {RelativeSource Self}}">

    <Grid Width="1080" Height="1920">
        <Image Source="../Images/страница выбранной фото.png"></Image>

        <Button
            Command="{Binding PrintCommand }" Margin="316,1451,316,355">
            <Button.Template>
                <ControlTemplate>
                    <Border Background="Transparent"/>
                </ControlTemplate>
            </Button.Template>
        </Button>
        <Image Source="../Images/1.jpg" x:Name="image" HorizontalAlignment="Left" Height="1208" Margin="44,200,0,0" VerticalAlignment="Top" Width="984"/>

    </Grid>
</Page>
